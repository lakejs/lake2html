{
  "name": "lake-html",
  "version": "2.0.0",
  "packageManager": "pnpm@10.28.1",
  "description": "A lightweight utility to convert Lake Markup Language (LML) to HTML",
  "author": "Luo Longhao <luolonghao@gmail.com>",
  "license": "MIT",
  "homepage": "http://lakejs.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lakejs/lake-html.git"
  },
  "bugs": {
    "url": "https://github.com/lakejs/lake-html/issues"
  },
  "keywords": [
    "rich text",
    "html",
    "converter"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "test": "vitest run --coverage",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "node ./release.mjs"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.2.0",
    "@vitest/coverage-v8": "^4.0.18",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.39.2",
    "execa": "^9.6.1",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "semver": "^7.7.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
